{
  "name": "@Neurodiversity-in-Business/neurodiversity-main",
  "version": "0.5.1",
  "author": "Usi Diamond",
  "homepage": "https://neurodiversity.org",
  "description": "Neurodiversity in Buisness (NiB) is a business-led forum functioning as an industry group for organisations to share industry good practice on ND recruitment, retention and empowerment.",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve",
    "start": "node server.js",
    "buildDev": "ng build --watch --configuration development",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch false",
    "coverage": "ng test --no-watch --code-coverage",
    "e2e": "webdriver-manager start && protractor ./src/test/protractor-config.js",
    "dockerBuild": "docker build -f Dockerfile -t neurodiversity-main .",
    "dockerRun": "docker run  --env=PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin --env=NODE_VERSION=14.21.3 --env=YARN_VERSION=1.22.19 --workdir=/ -p 8080:8080 --runtime=runc -d neurodiversity-main:latest"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/platform-browser-dynamic": "^16.1.4",
    "@angular/router": "^16.1.4",
    "@ng-bootstrap/ng-bootstrap": "^15.1.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.3.0",
    "express-rate-limit": "^6.7.0",
    "express.js": "^1.0.0",
    "jquery": "^3.7.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@angular/localize": "^16.1.4",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5"
  },
  "publishConfig": {
    "registry": "https://github.com/Neurodiversity-in-Business"
  }
}
